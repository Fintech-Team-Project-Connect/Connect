	<script>
        // http:// , https://, ftp:// .. etc 로 시작하지 않을 경우 자동으로 붙여주는 함수
        function getRightURL(n) {
            var tmpURL = n.replace(" ", "")
            var tmp = tmpURL.toLowerCase();
            if( tmp.indexOf("http://") == 0 ||
                tmp.indexOf("https://") == 0 ||
                tmp.indexOf("ftp://") == 0 ||
                tmp.indexOf("mailto:// ") == 0 ||
                tmp.indexOf("mms://") == 0 ||
                tmp.indexOf("pnm://") == 0 ||
                tmp.indexOf("telnet:// ") == 0 ||
                tmp.indexOf("rlogin://") == 0 ||
                tmp.indexOf("news:// ") == 0 ||
                tmp.indexOf("file://") == 0 )
                return tmpURL;
            else
                return "http://"+tmpURL ;
        }

        $("#enterBtn").click(function() {
            // alert($("#urlValue").val());
            var url = getRightURL($("#urlValue").val());
            window.location = url;
		})
        var jwtToken = sessionStorage.getItem('jwtToken')
        fetchUserData();
        function fetchUserData(){
            $.ajax({
                url:'http://localhost:3000/joinStudy',
                type : 'POST',
                headers : {
                    'x-access-token' : jwtToken
                }
            })

        }
    </script>
